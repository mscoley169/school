package edu.nashcc.circle;

public class TestCircle {

	public static void main(String[] args) {

		//instantiating circles
		Circle cir1 = new Circle(3);
		Circle cir2 = new Circle(120);
		Circle cir3 = new Circle();
		Circle cir4 = new Circle();
		
		//demonstrating setter for radius
		cir4.setRadius(5.5);
		
		//displaying data with String.format
		cir1.getFormattedData(cir1);
		cir2.getFormattedData(cir2);
		cir3.getFormattedData(cir3);
		cir4.getFormattedData(cir4);
		
		
	}

}
