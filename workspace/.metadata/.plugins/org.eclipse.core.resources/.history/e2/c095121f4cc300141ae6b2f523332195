package edu.nashcc.paint;

import java.util.Scanner;

public class TestPainting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Painting paint1 = new Painting("", "", "", 0.0);
		Painting paint2 = new Painting("", "","", 0.0);
		Painting paint3 = new Painting("Starry Night", "Van Gogh", "oil", 100000000);
		
		paint1.setUserInput(paint1);
		paint2.setUserInput(paint2);
		
		//testing stuff
		System.out.println(paint1);
		System.out.println(paint2);
		System.out.println(paint3);
			
	}

	
}
